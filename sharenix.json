{
    "DefaultFileUploader": "hnng.moe (file upload)",
    "DefaultImageUploader": "imgur.com",
    "DefaultUrlShortener": "hnng.moe (url shortener)",

    "Services": [
        {
            "Name": "hnng.moe (url shortener)",
            "RequestType": "GET",
            "RequestURL": "http://www.hnng.moe/shortapi.php",
            "Arguments": {
                "url": "$input$"
            },
            "ResponseType": "Text",
            "RegexList": []
        },
        {
            "Name": "hnng.moe (file upload)",
            "RequestType": "POST",
            "RequestURL": "http://www.hnng.moe/upload_d.php",
            "FileFormName": "file",
            "ResponseType": "Text",
            "URL": "$json:url$",
            "DeletionURL": "$json:deletelink$"
        },
        {
            "Name": "imgur.com",
            "RequestType": "POST",
            "Headers": {
                "Authorization": "Client-ID b972ecca954f246"
            },
            "RequestURL": "https://api.imgur.com/3/image",
            "FileFormName": "image",
            "Arguments": {
                "type": "file"
            },
            "ResponseType": "Text",
            "URL": "$json:data.link$",
            "DeletionURL": "https://imgur.com/delete/$json:data.deletehash$"
        }
    ]
}
